/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
     not(in=b,out=notb)
     And(a=a,b=notb,out=x1)
     Not(in=a,out=nota)
     And(a=nota,b=b,out=x2)
     Or(a=x1,b=x2,out=out)
}